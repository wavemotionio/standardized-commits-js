    "url": "https://github.com/wavemotionio/standardized-commits-js/issues"
  },
  "homepage": "https://github.com/wavemotionio/standardized-commits-js#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "husky": "^1.3.1",
    "semantic-git-commit-cli": "^3.2.1"
  }
