{
  "name": "standardized-commits-js",
  "version": "1.0.0",
  "description": "An example of standardizing commit messages with conventional-commits, commitlint, and config-conventional",
  "scripts": {
    "commit": "node_modules/.bin/sgc",
    "test": "./node_modules/.bin/eslint ./"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wavemotionio/standardized-commits-js.git"
  },
  "keywords": [
    "commitlint",
    "conventional-commits",
    "standardized-commits",
    "semantic-git-commit-cli"
  ],
  "author": "wavemotionio",
  "license": "ISC",
  "homepage": "https://github.com/wavemotionio/standardized-commits-js#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "eslint": "^5.15.3",
    "husky": "^1.3.1",
    "semantic-git-commit-cli": "^3.2.1"
  }
}
